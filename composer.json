{
    "name": "maastermedia/whois-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 whois queries bundle",
    "keywords": ["whois"],
    "homepage": "http://github.com/maastermedia/MmWhoisBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Kokot",
            "email": "peterkokot@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "symfony/form": "~2.1",
        "symfony/security-bundle": "~2.1"
    },
    "require-dev": {
        "twig/twig": "~1.5",
        "doctrine/doctrine-bundle": "*",
        "swiftmailer/swiftmailer": ">=4.3, <6.0",
        "symfony/yaml": "~2.1",
        "symfony/validator": "~2.1"
    },
    "autoload": {
        "psr-0": { "Mm\\WhoisBundle": "" }
    },
    "target-dir": "Mm/WhoisBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "*"
        }
    }
}
